<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Lab1</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    
</head>
<body>
<header>
    <h1>JavaJam Coffee House</h1>
    <nav>
        <ul>
            <li>
                <a asp-controller="Home" asp-action="Index" id="musicLink">Home</a>
            </li>
            <li>
                <a asp-controller="Home" asp-action="Menu" >Menu</a>
            </li>
            <li>
                <a asp-controller="Home" asp-action="Music" >Music</a>
            </li>
            <li>
                <a asp-controller="Home" asp-action="Jobs">Jobs</a>
            </li>
        </ul>
    </nav>
</header>
    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer>
        <div>
            Copyright &copy; 2020 JavaJam Coffee House
            <br>
            <a href="mailto:JavaJam@nowhere.com">JavaJam@nowhere.com</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
